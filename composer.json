{
  "name": "setrun/setrun-app-basic",
  "description": "SETRUN CMS basic application based on php framework Yii2",
  "keywords": ["yii2", "yii2-extension", "yii2-cms", "cms", "setrun", "setrun-cms", "setrun-basic-app"],
  "homepage": "http://setrun.ru",
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name" : "Alexey Bashkov",
      "email": "ab@smpage.ru",
      "role" : "Ui Ux Designer"
    },
    {
      "name" : "Denis Utkin",
      "email": "dizirator@gmail.com",
      "role" : "Developer",
      "homepage": "http://github.com/dizirator"
    }
  ],
  "require": {
    "setrun/setrun-sys-component"     : "@dev",
    "setrun/setrun-user-component"    : "@dev",
    "setrun/setrun-backend-component" : "@dev",
    "setrun/setrun-content-component" : "@dev"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.0.0",
    "yiisoft/yii2-gii": "~2.0.0",
    "yiisoft/yii2-faker": "~2.0.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "name": "yiidoc/yii2-timeago",
      "type": "vcs",
      "url": "git@github.com:dizirator/yii2-timeago.git"
    }
  ],
  "config": {
    "process-timeout": 1800
  },
  "scripts": {
    "post-install-cmd": [
      "./composer-post-install"
    ],
    "post-update-cmd": [
      "./composer-post-update"
    ]
  }
}
